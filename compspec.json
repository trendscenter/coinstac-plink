{
  "meta": {
    "name": "Plink Computatiion in COINSTAC",
    "id": "coinstac-plink",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/trendscenter\/coinstac-plink.git",
    "description": "A way to use plink genetic analysis in COINSTAC.",
    "compspecVersion": 2
  },
  "computation": {
    "type": "docker",
    "dockerImage": "coinstacteam/coinstac-plink",
    "remote": {
      "type": "docker",
      "dockerImage": "coinstacteam/coinstac-plink"
    },
    "input": {
      "command": {
        "default": "./plink",
        "label": "Main command",
        "type": "string",
        "source": "owner",
        "order": 1
      },
      "data": {
        "label": "Data",
        "type": "files",
        "items": [
          "Files"
        ],
        "extensions": [
          "map",
          "ped"
        ],
        "source": "member",
        "order": 2
      },
      "command_args": {
        "default": {
          "--file": "toy",
          "--freq": "",
          "--out": "analysis_result"
        },
        "label": "Provide plink arguments as below",
        "type": "object",
        "source": "owner",
        "order": 3
      }
    },
    "output": {
      "analysis": {
        "type": "string",
        "label": "Results"
      }
    },
    "display": {
      "type": "string"
    }
  }
}